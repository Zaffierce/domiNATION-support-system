<%- include('../../partials/head.ejs'); %>
  <title>User: <%= user.username %> - DomiNATION Ark: Survival Evolved Community</title>
<link rel="stylesheet" type="text/css" href="styles/forms.css">
<script src="/js/app.js" type="text/javascript"></script>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-validator/0.5.3/css/bootstrapValidator.min.css" integrity="sha256-gBxd65TVOCRCDC48+AXN6hhg3I/fZqE/BnwgKwdzJG4=" crossorigin="anonymous" />
<script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-validator/0.5.3/js/bootstrapValidator.min.js" integrity="sha256-PbFF1Mdg86urwOYXWNJPP4z5Ge9KLp6KXX1NURQY8Ho=" crossorigin="anonymous"></script>
</head>
<body>
  <%- include('../../partials/nav.ejs'); %>
  <br />
  <div class="container">
    <div class="row">
      <div class="col-x1-5 col-lg-6 col-md-8 col-sm-10 mx-auto text-center form p-4">
        <h1 class="ng-scope text-center">Please choose an option</h1>
        <div class="px-2">
          <form class="needs-validation justify-content-center" action="creating-ticket" method="POST">
            <%- include('../../partials/completedGeneralTicket.ejs'); %>
            <div class="form-group">
              <label for="ticketType">Which option are you requesting?</label>
              <select name="ticketType" class="form-control mx-auto" id="generalTicketSelect" required>
                <option value="Patreon Monthly Dino">Monthly Dino Patreon Request</option>
                <option value="Patreon Insurance Request">Dino Patreon Insurance Request</option>
              </select>
            </div>
            <div class="text-center">
              <input type="submit" value="Continue">
            </div>
          </form>
        </div>
      </div>
    </div>
  </div>
</body>
</html>