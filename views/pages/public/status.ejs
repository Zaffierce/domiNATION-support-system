<%- include('../../partials/head.ejs'); %>
  <title>User: <%= user.username %> - DomiNATION Ark: Survival Evolved Community</title>
<link rel="stylesheet" type="text/css" href="../../styles/forms.css">
<script src="/js/app.js" type="text/javascript"></script>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-validator/0.5.3/css/bootstrapValidator.min.css" integrity="sha256-gBxd65TVOCRCDC48+AXN6hhg3I/fZqE/BnwgKwdzJG4=" crossorigin="anonymous" />
<script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-validator/0.5.3/js/bootstrapValidator.min.js" integrity="sha256-PbFF1Mdg86urwOYXWNJPP4z5Ge9KLp6KXX1NURQY8Ho=" crossorigin="anonymous"></script>
</head>
<body>
  <%- include('../../partials/nav.ejs'); %>
  <h1 class="ng-scope text-center">Welcome to your profile, <%= user.username %>!</h1>
  <div class="container">
    <table class="table table-striped">
      <tbody>
        <tr>
          <th class="ng-scope">Date Submitted</th>
          <th class="ng-scope">Ticket Type</th>
          <th class="ng-scope">Status</th>
          <th class="ng-scope">ID</th>
          <th class="ng-scope">Date Completed</th>
        </tr>
        <% for (let i = 0; i < myTickets.length; i++) { %>
        <tr class="ng-scope">
          <td class="ng-scope"><%=myTickets[i].submitted_on%></td>
          <td class="ng-scope"><%=myTickets[i].type_of_ticket%></td>
          <td class="ng-scope"><%=myTickets[i].status%></td>
          <td class="ng-scope"><a href="/details/<%=myTickets[i].id%>">View ticket</a></td>
          <td class="ng-scope"><%=myTickets[i].closed_on%></td>
        </tr>
        <% } %>
      </tbody>
    </table>
  </div>
</div> <!--Close of content-->

</body>
</html>